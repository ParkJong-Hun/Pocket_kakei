<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parkjonghun.pocket_kakei">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_add_money"
        android:label="@string/display_app_name"
        android:roundIcon="@drawable/ic_add_money"
        android:supportsRtl="true"
        android:theme="@style/Theme.Kaji_pocket">
        <activity
            android:name=".view.activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.AddActivity"/>
        <activity android:name=".view.activity.SheetActivity"/>
    </application>

</manifest>